{
  "id": "cspt",
  "name": "财神",
  "isShow": true,
  "baseUrls": [
    "https://cspt.top/",
    "https://cspt.cc/",
    "https://cspt.date/"
  ],
  "primaryUrl": "https://cspt.top/",
  "siteType": "NexusPHPWeb",
  "searchCategories": [],
  "features": {
    "userProfile": true,
    "torrentSearch": true,
    "torrentDetail": true,
    "download": true,
    "favorites": true,
    "downloadHistory": true,
    "categorySearch": true,
    "advancedSearch": true,
    "showCover": true
  },
  "discountMapping": {},
  "infoFinder": {
    "userInfo": {
      "path": "usercp.php",
      "rows": {
        "selector": "div#user-info-pannl"
      },
      "fields": {
        "userId": {
          "selector": "a[href^=\"userdetails.php?id=\"]",
          "attribute": "href",
          "filter": {
            "name": "regexp",
            "args": "id=(\\d+)",
            "value": "$1"
          }
        },
        "userName": {
          "selector": "span.user-id > a[href^=\"userdetails.php?id=\"] > b",
          "attribute": "text"
        },
        "ratio": {
          "selector": "div.menu-base-info",
          "attribute": "text",
          "filter": {
            "name": "regexp",
            "args": "分享率(:|：)\\s*([^\\s]+)",
            "value": "$2"
          }
        },
        "upload": {
          "selector": "div.menu-base-info",
          "attribute": "text",
          "filter": {
            "name": "regexp",
            "args": "(上传|上傳)量(:|：)\\s*(.*?B)",
            "value": "$3"
          }
        },
        "download": {
          "selector": "div.menu-base-info",
          "attribute": "text",
          "filter": {
            "name": "regexp",
            "args": "(下载|下載)量(:|：)\\s*(.*?B)",
            "value": "$3"
          }
        },
        "bonus": {
          "selector": "div.menu-base-info > a[href^=\"mybonus.php\"]",
          "attribute": "text"
        }
      }
    },
    "categories": {
      "path": "usercp.php?action=tracker",
      "rows": {
        "selector": "#outer > table.main > next > tbody:nth-child > tr:nth-child(1) > td.rowfollow > table"
      },
      "fields": {
        "categoryId": {
          "selector": "a[href~=\"(torrents|special)\\.php\"]",
          "attribute": "href",
          "filter": {
            "name": "regexp",
            "args": "cat=(\\d+)",
            "value": "$1"
          }
        },
        "categoryName": {
          "selector": "a[href~=\"(torrents|special)\\.php\"] > img",
          "attribute": "title"
        }
      }
    },
    "search": {
      "rows": {
        "selector": "div.torrents > div:nth-child(2) > div:nth-child"
      },
      "fields": {
        "torrentId": {
          "selector": "div.torrent-title > a[href^=\"details.php?id=\"]",
          "attribute": "href",
          "filter": {
            "name": "regexp",
            "args": "id=(\\d+)",
            "value": "$1"
          }
        },
        "torrentName": {
          "selector": "div.torrent-title > a[href^=\"details.php?id=\"] > b",
          "attribute": "text"
        },
        "tag": {
          "selector": "div.torrent-title > span[title]",
          "attribute": "text"
        },
        "description": {
          "selector": "div.torrent-title > div.torrent-info-text-small_name",
          "attribute": "text"
        },
        "discount": {
          "selector": "div.torrent-title > img[class^=\"pro_\"]",
          "attribute": "alt"
        },
        "discountEndTime": {
          "selector": "div.torrent-title > img[class^=\"pro_\"]",
          "attribute": "onmouseover",
          "filter": {
            "name": "regexp",
            "args": "<span title=\\\"(.*?)\\\"",
            "value": "$1"
          },
          "time": {
            "format": "yyyy-MM-dd HH:mm:ss",
            "zone": "+08:00"
          }
        },
        "downloadUrl": {},
        "seedersText": {
          "selector": "div.torrent-info-text-seeders > a",
          "attribute": "text"
        },
        "leechersText": {
          "selector": "div.torrent-info-text-leechers > a",
          "attribute": "text"
        },
        "sizeText": {
          "selector": "div.torrent-info-text-size",
          "attribute": "text"
        },
        "downloadStatus": {
          "selector": "div.torrent-title > div:nth-child > div:nth-child(4) > div[title]",
          "attribute": "title",
          "filter": {
            "name": "regexp",
            "args": "(\\d+)%",
            "value": "$1"
          }
        },
        "collection": {
          "selector": "div.torrent-manage > img.bookmark",
          "attribute": "src"
        },
        "isTop": {
          "selector": "table.torrentname > img[alt=\"Sticky\"]",
          "attribute": "src"
        },
        "cover": {},
        "createDate": {
          "selector": "div.torrent-info-text-added > span",
          "attribute": "title",
          "time": {
            "format": "yyyy-MM-dd HH:mm:ss",
            "zone": "+08:00"
          }
        },
        "doubanRating": {
          "selector": "div.torrent-title > img[title==\"douban\"] > next",
          "attribute": "text"
        },
        "imdbRating": {
          "selector": "div.torrent-title > img[title==\"imdb\"] > next",
          "attribute": "text"
        }
      }
    }
  },
  "logo": "assets/sites_icon/cspt.png"
}