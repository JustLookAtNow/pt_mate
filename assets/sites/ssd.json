{
  "id": "ssd",
  "name": "SSD",
  "isShow": true,
  "baseUrls": [
    "https://springsunday.net/"
  ],
  "primaryUrl": "https://springsunday.net/",
  "siteType": "NexusPHPWeb",
  "searchCategories": [],
  "features": {
    "userProfile": true,
    "torrentSearch": true,
    "torrentDetail": true,
    "download": true,
    "favorites": true,
    "downloadHistory": true,
    "categorySearch": true,
    "advancedSearch": true,
    "showCover": false
  },
  "discountMapping": {},
  "tagMapping": {
      "官方": "official",
      "中字": "chinese",
      "国配": "mandarin",
      "DIY": "diy",
      "菁彩HDR":"hdr",
      "HDR10":"hdr",
      "HLG":"hdr",
      "HDR10+":"hdr",
      "DoVi":"dovi",
      "合集": "complete"
   },
  "infoFinder": {
     "userInfo": {
          "path": "usercp.php",
          "rows": {
            "selector": "div#info_block > div:nth-child(1)"
          },
          "fields": {
            "userId": {
              "selector": "a[href^=\"userdetails.php?id=\"]",
              "attribute": "href",
              "filter": {
                "name": "regexp",
                "args": "id=(\\d+)",
                "index": 1
              }
            },
            "userName": {
              "selector": "a[href^=\"userdetails.php?id=\"] > b",
              "attribute": "text"
            },
            "ratio": {
              "selector": "",
              "attribute": "text",
              "filter": {
                "name": "regexp",
                "args": "分享率(:|：)\\s*([^\\s]+)",
                "index": 2
              }
            },
            "upload": {
              "selector": "",
              "attribute": "text",
              "filter": {
                "name": "regexp",
                "args": "(上传|上傳)量(:|：)\\s*(.*?B)",
                "index": 3
              }
            },
            "download": {
              "selector": "",
              "attribute": "text",
              "filter": {
                "name": "regexp",
                "args": "(下载|下載)量(:|：)\\s*(.*?B)",
                "index": 3
              }
            },
            "bonus": {
              "selector": "a[href^=\"mybonus.php\"]",
              "attribute": "title",
              "filter": {
                "name": "regexp",
                "args": "茉莉(:|：)\\s*([^\\s]+)\\s*",
                "index": 2
              }
            }
          }
        },
        "passKey": {
          "path": "usercp.php",
          "rows": {
          "selector": "div#outer > div.main > next > tbody >td:contains('密匙' || '密钥' || 'Passkey') > next"
          },
          "fields": {
            "passKey": {
              "selector": "",
              "attribute": "text"
            }
          }
        },
        "bonusPerHour": {
          "path": "mybonus.php",
          "rows": {
            "selector": "#outer"
          },
          "fields": {
            "bonusPerHour": {
              "selector": "div.main > div:nth-child(4) > div.section-text > table:nth-child(4) > tbody > tr:nth-child(1) > td:nth-child(11)",
              "attribute": "text"
            }
          }
        },
    "search": {
      "torrentPath": "torrents.php",
      "specialPath": "special.php",
      "rows": {
        "selector": "table.torrents > tbody:nth-child(1) > tr:nth-child"
      },
      "fields": {
        "torrentId": {
          "selector": "a[href^=\"details.php?id=\"]",
          "attribute": "href",
          "filter": {
            "name": "regexp",
            "args": "id=(\\d+)",
            "index": 1
          }
        },
        "torrentName": {
          "selector": "a[href^=\"details.php?id=\"]",
          "attribute": "text"
        },
        "tag": {
          "selector": "table.torrentname > div.torrent-smalldescr > span.torrent-tag",
          "attribute": "text"     
        },
        "description": {
          "selector": "table.torrentname > div.torrent-smalldescr > span:nth-child",
          "attribute": "text"
        },
        "discount": {
          "selector": "table.torrentname > span[class~=\"torrent-pro\"]",
          "attribute": "text"
        },
        "discountEndTime": {
          "selector": "table.torrentname > span[class~=\"torrent-pro\"] > next > span",
          "attribute": "title"
        },
        "downloadUrl": {
          "value": "{baseUrl}/download.php?id={torrentId}&passkey={passKey}&https=1"
        },
        "seedersText": {
          "selector": "td:nth-child(6)",
          "attribute": "text"
        },
        "leechersText": {
          "selector": "td:nth-child(7)",
          "attribute": "text"
        },
        "sizeText": {
          "selector": "td:nth-child(5)",
          "attribute": "text"
        },
        "downloadStatus": {
          "selector": "table.torrentname > tbody > tr > td:nth-child(1) > div.progress_bar",
          "attribute": "title",
          "filter": {
            "name": "regexp",
            "args": "(\\d+)%",
            "index": 1
          }
        },
        "collection": {
          "selector": "table.torrentname > tbody > tr > span.bi-star-fill",
          "attribute": "title"
        },
        "isTop": {
          "selector": "table.torrentname > span[title~=\"置顶|置頂\"]",
          "attribute": "title"
        },
        "createDate": {
          "selector": "td:nth-child(4) > span[title]",
          "attribute": "title"
        },
        "doubanRating": {
          "selector": "table.torrentname > img[alt==\"douban\"] > next",
          "attribute": "text"
        },
        "imdbRating": {
          "selector": "table.torrentname > img[alt==\"imdb\"] > next",
          "attribute": "text"
        }
      }
    },
    "categories": {
      "path": "usercp.php?action=tracker",
      "rows": {
        "selector": "#outer > tbody > tr:nth-child(1) > td.rowfollow > table:nth-child(1) > tbody > tr:nth-child(2)"
      },
      "fields": {
        "categoryId": {
          "selector": "a[href~=\"(torrents|special)\\.php.*\"]",
          "attribute": "href",
          "filter": {
            "name": "regexp",
            "args": "cat=(\\d+)",
            "index": 1
          }
        },
        "categoryName": {
          "selector": "a[href~=\"(torrents|special)\\.php\"] > img",
          "attribute": "title"
        }
      }
    }
  },
  "logo": "assets/sites_icon/ssd.png"
}
