{
  "id": "nicept",
  "name": "老师",
  "isShow": false,
  "baseUrls": [
    "https://www.nicept.net/"
  ],
  "primaryUrl": "https://www.nicept.net/",
  "siteType": "NexusPHPWeb",
  "searchCategories": [],
  "features": {
    "userProfile": true,
    "torrentSearch": true,
    "torrentDetail": true,
    "download": true,
    "favorites": true,
    "downloadHistory": true,
    "categorySearch": true,
    "advancedSearch": true,
    "nativeDetail": true
  },
  "discountMapping": {},
  "infoFinder": {
    "categories": {
      "path": "usercp.php?action=tracker",
      "rows": {
        "selector": "#outer > form > table > tbody > tr:nth-child(1) > td.rowfollow > table"
      },
      "fields": {
        "categoryId": {
          "selector": "a[href~=\"(torrents|special)\\.php\"]",
          "attribute": "href",
          "filter": {
            "name": "regexp",
            "args": "cat=(\\d+)",
            "value": "$1"
          }
        },
        "categoryName": {
          "selector": "a[href~=\"(torrents|special)\\.php\"] > img",
          "attribute": "title"
        }
      }
    },
    "detail": {
      "isBbcode": false,
      "rows": {
        "selector": "div#kdescr"
      }
    }
  },
  "logo": "assets/sites_icon/nicept.png"
}